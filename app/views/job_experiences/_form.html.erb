<%= simple_form_for(@job_experience) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :company %>
    <%= f.input :job_title %>
    <%= f.input :description %>
    <%= f.input :start_date %>
  </div>

  <p>job experiences form</p>
  <div class="form-actions">
    <%= f.button :submit %>
  </div>

<% end %>
